#include <iostream>
#include <vector>
using namespace std;

//fedeo46 
int main()
{
    vector<int>vNumber = { 1,2,3,4,5 };
    
    cout << "\n\nUsing .at(1) \n";
    cout <<"Elements at Index 0 " << vNumber.at(0)<<endl;
    cout << "Elements at Index 2 " << vNumber.at(2)<<endl;
    cout << "Elements at Index 4 " << vNumber.at(4)<<endl;
   // cout << "Elements at Index 5 " << vNumber.at(5);
   

    cout << "\n\nUsing [1] \n";
    cout << "Elements at Index 0 " << vNumber[0]<<endl;
    cout << "Elements at Index 2 " << vNumber[2]<<endl;
    cout << "Elements at Index 4 " << vNumber[4]<<endl;
    //cout << "Elements at Index 5 " << vNumber[5];

}
